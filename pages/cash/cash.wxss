.container{background: #F5F6FF;margin-top:128rpx;}
.cash{width:622rpx;background: #FFFFFF;
box-shadow: 0 3px 4px 0 rgba(55,89,203,0.10);
border-radius: 8rpx;height: 240rpx;margin:0 auto;margin-top: 40rpx;padding:56rpx 32rpx 0 32rpx;}
.cash text{font-family: PingFangSC-Regular;
font-size: 14px;
color: #666666;
letter-spacing: 0;
text-align: left;
line-height: 14px;padding-bottom:20rpx;display: block;}
.cash input{font-family: ArialMT;
font-size: 80rpx;
color: #0052CC;
letter-spacing: 0;
text-align: left;
line-height: 80rpx;height:80rpx;}
.address{width:670rpx;margin:100rpx auto;color:#999999;font-size:32rpx;}
.address input{padding:28rpx;border-bottom: 1px solid #CBD5E3;}
.pushbutton{width:670rpx;
text-align:center;
position:absolute;
bottom:40rpx;
left:40rpx;
line-height: 96rpx;
font-size:36rpx;
height:96rpx;
letter-spacing: 0;color:#fff;background:none;}
button[disabled]:not([type]) {color:#fff;
opacity: .5;
border-radius: 4rpx;
}
.gcash image{width:670rpx;
background-size:100%;
height:96rpx;
position:absolute;
left:0;
}
.pushbutton i{position: absolute;z-index: 1;left:0;right:0;top:0;bottom:0;}
